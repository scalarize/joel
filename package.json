{
	"name": "joel",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "npm run deploy:worker && npm run deploy:pages",
		"dev": "cd api && npx wrangler dev",
		"start": "cd api && npx wrangler dev",
		"test": "cd api && vitest",
		"cf-typegen": "cd api && npx wrangler types",
		"deploy:worker": "cd api && npx wrangler deploy",
		"deploy:pages": "cd web && npm run build && npx wrangler pages deploy dist --project-name joel-pages"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.55.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.958.0",
		"sharp": "^0.34.5"
	}
}
